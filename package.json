{
  "name": "react-headstarter",
  "version": "0.0.1",
  "description": "React starter kit",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=src/ react-app-rewired start",
    "build": "NODE_PATH=src/ react-app-rewired build",
    "test": "NODE_PATH=src/ react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "styleguide": "NODE_PATH=src/ npx styleguidist server",
    "styleguide:build": "NODE_PATH=src/ npx styleguidist build",
    "precommit": "lint-staged",
    "lint": "./node_modules/.bin/eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://selrond@github.com/selrond/react-headstarter.git"
  },
  "keywords": [
    "react",
    "styled-components",
    "atomic-design",
    "eslint",
    "starter-kit",
    "boilerplate",
    "react-router",
    "styleguide"
  ],
  "author": "Sebastian Andil <sebastian.andil@gmail.com> (https://twitter.com/selrond)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/selrond/react-headstarter/issues"
  },
  "homepage": "https://github.com/selrond/react-headstarter#readme",
  "dependencies": {
    "normalize.css": "^8.0.0",
    "polished": "^2.2.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^2.0.5",
    "sanitize.css": "^8.0.0",
    "styled-components": "^4.0.2",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.8.0",
    "docz": "^0.12.5",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "react-app-rewired": "^1.6.2"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  }
}
