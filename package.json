{
  "name": "react-headstarter",
  "version": "0.0.1",
  "description": "React starter kit",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=src/ react-scripts start",
    "build": "NODE_PATH=src/ react-scripts build",
    "test": "NODE_PATH=src/ react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "styleguide": "NODE_PATH=src/ npx styleguidist server",
    "styleguide:build": "NODE_PATH=src/ npx styleguidist build",
    "precommit": "lint-staged",
    "lint": "./node_modules/.bin/eslint --fix ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://selrond@github.com/selrond/react-headstarter.git"
  },
  "keywords": [
    "react",
    "styled-components",
    "atomic-design",
    "eslint",
    "starter-kit",
    "boilerplate",
    "react-router",
    "styleguide"
  ],
  "author": "Sebastian Andil <sebastian.andil@gmail.com> (https://twitter.com/selrond)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/selrond/react-headstarter/issues"
  },
  "homepage": "https://github.com/selrond/react-headstarter#readme",
  "dependencies": {
    "normalize.css": "^8.0.0",
    "polished": "^1.9.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "sanitize.css": "^5.0.0",
    "styled-components": "^3.3.0",
    "styled-system": "^2.2.5",
    "system-components": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "react-styleguidist": "^7.0.14"
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  }
}
