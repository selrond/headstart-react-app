  "author": "Sebastian Andil <sebastian.andil@gmail.com> (https://twitter.com/selrond)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/selrond/headstart-react-app/issues"
  },
  "homepage": "https://github.com/selrond/headstart-react-app#readme",
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
